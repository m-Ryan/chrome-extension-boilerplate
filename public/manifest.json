{
  "manifest_version": 3,
  "name": "Chrome插件脚手架",
  "version": "0.0.1",
  "description": "Chrome插件脚手架",
  "author": "m-Ryan",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },

  "background": {
    "service_worker": "static/js/background.js"
  },
  "content_scripts": [
    {
      "js": ["static/js/web.js"],
      "css": [],
      "matches": ["*://*/*"]
    }
  ],
  "commands": {},
  "permissions": [
    "desktopCapture",
    "activeTab",
    "tabCapture",
    "cookies",
    "contextMenus", // 右键菜单
    "tabs", // 标签
    "notifications", // 通知
    "storage" // 插件本地存储]
  ],
  "host_permissions": ["<all_urls>"]
}
